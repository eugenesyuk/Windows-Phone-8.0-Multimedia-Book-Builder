<Window x:Class="BookGen.Windows.SoundsForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SoundsForm" Height="156" Width="394">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="30,15,0,0" Name="lblSound" Text="Sound" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,44,0,0" Name="lblLanguage" Text="Language" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="70,12,0,0" Name="txtSound" VerticalAlignment="Top" Width="243" PreviewMouseUp="txtSound_PreviewMouseUp" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="70,44,0,0" Name="txtLanguage" VerticalAlignment="Top" Width="148" />
        <Button Content="..." Height="23" HorizontalAlignment="Left" Margin="319,12,0,0" Name="btnBrowseSound" VerticalAlignment="Top" Width="34" Click="btnBrowseSound_Click" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="130,83,0,0" Name="btnOk" VerticalAlignment="Top" Width="75" Click="btnOk_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,83,86,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
