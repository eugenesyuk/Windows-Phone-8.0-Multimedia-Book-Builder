<Window x:Class="BookGen.Windows.LanguagesForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LanguagesForm" Height="161" Width="425">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,12,0,0" Name="lblImage" Text="Image" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,44,0,0" Name="lblLanguage" Text="Language" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="85,9,0,0" Name="txtImage" VerticalAlignment="Top" Width="266" PreviewMouseUp="txtImage_PreviewMouseUp" />
        <TextBox Height="23" Margin="85,41,102,0" Name="txtLanguage" VerticalAlignment="Top" />
        <Button Content="..." Height="23" HorizontalAlignment="Right" Margin="0,9,12,0" Name="btnBrowseImage" VerticalAlignment="Top" Width="31" Click="btnBrowseImage_Click" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="132,82,0,0" Name="btnOk" VerticalAlignment="Top" Width="75" Click="btnOk_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="213,82,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
