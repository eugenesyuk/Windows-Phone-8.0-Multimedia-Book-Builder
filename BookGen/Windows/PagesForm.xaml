<Window x:Class="BookGen.Windows.PagesForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PagesForm" Height="156" Width="326">
    <Grid>
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="lblPageName" Text="Name" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="14,41,0,0" Name="lblDelay" Text="Delay" VerticalAlignment="Top" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="60,12,0,0" Name="txtPageName" VerticalAlignment="Top" Width="231" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="60,41,0,0" Name="txtDelay" VerticalAlignment="Top" Width="93" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="60,83,0,0" Name="btnOk" VerticalAlignment="Top" Width="75" Click="btnOk_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Right" Margin="0,83,84,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" />
    </Grid>
</Window>
